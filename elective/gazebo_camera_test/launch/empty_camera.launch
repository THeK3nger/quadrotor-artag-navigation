<launch>

  <!--<arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="throttled" default="false"/>-->

  <!-- set use_sim_time flag -->
  <param name="/use_sim_time" value="true" />

  <!-- start empty world -->
  <node name="gazebo" pkg="gazebo" type="gazebo" args="-u $(find gazebo_worlds)/worlds/empty.world" respawn="false" output="screen"/>

 
   <node name="tag" pkg="gazebo" type="spawn_model" args="-file $(find gazebo_camera_test)/urdf/ARtag.urdf -urdf -z 1 -model ARtag" respawn="false" />

  
  <!-- start gui -->
  <node name="gazebo_gui" pkg="gazebo" type="gui" respawn="false" output="screen"/>



</launch>
